{
  "sourceRoot": "apps/kontent-custom-element/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nrwl/web:webpack",
      "outputs": ["{options.outputPath}"],
      "options": {
        "compiler": "swc",
        "outputPath": "dist/apps/kontent-custom-element",
        "index": "apps/kontent-custom-element/src/index.html",
        "main": "apps/kontent-custom-element/src/main.tsx",
        "polyfills": "apps/kontent-custom-element/src/polyfills.ts",
        "tsConfig": "apps/kontent-custom-element/tsconfig.app.json",
        "assets": ["apps/kontent-custom-element/src/favicon.ico"],
        "styles": ["apps/kontent-custom-element/src/styles.scss"],
        "scripts": [],
        "webpackConfig": "@nrwl/react/plugins/webpack"
      },
      "configurations": {
        "production": {
          "fileReplacements": [
            {
              "replace": "apps/kontent-custom-element/src/environments/environment.ts",
              "with": "apps/kontent-custom-element/src/environments/environment.prod.ts"
            }
          ],
          "optimization": true,
          "outputHashing": "all",
          "sourceMap": false,
          "extractCss": true,
          "namedChunks": false,
          "extractLicenses": true,
          "vendorChunk": false,
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "2mb",
              "maximumError": "5mb"
            }
          ]
        },
        "development": {
          "extractLicenses": false,
          "optimization": false,
          "sourceMap": true,
          "vendorChunk": true
        }
      },
      "defaultConfiguration": "production"
    },
    "serve": {
      "executor": "@nrwl/web:dev-server",
      "options": {
        "buildTarget": "kontent-custom-element:build",
        "hmr": true
      },
      "configurations": {
        "production": {
          "buildTarget": "kontent-custom-element:build:production",
          "hmr": false
        },
        "development": {
          "buildTarget": "kontent-custom-element:build:development"
        }
      },
      "defaultConfiguration": "development"
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": ["apps/kontent-custom-element/**/*.{ts,tsx,js,jsx}"]
      },
      "outputs": ["{options.outputFile}"]
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/apps/kontent-custom-element"],
      "options": {
        "jestConfig": "apps/kontent-custom-element/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "deploy": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "commands": [
          "npx gzipper c --gzip --gzip-level 9 --output-file-format [filename].[ext] ./dist/apps/kontent-custom-element ./dist/apps/kontent-custom-element/build-gz",
          "aws s3 sync ./dist/apps/kontent-custom-element/build-gz s3://kontent-custom-${AWS_ACCOUNT_ID} --delete --metadata-directive REPLACE --cache-control max-age=31536000,s-maxage=31536000 --content-encoding gzip --acl public-read"
        ],
        "parallel": false
      }
    }
  },
  "tags": []
}
